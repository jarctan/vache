fn fibo(i: int) -> int {
    if i == 0 {
        0
    } else if i == 1 {
        1
    } else {
        fibo(i - 1) + fibo(i - 2)
    }
}

fn main() {
    print(fibo(0));
    print(fibo(1));
    print(fibo(2));
    print(fibo(11));
}
