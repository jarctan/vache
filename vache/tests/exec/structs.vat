struct Pos {
    x: int,
    y: str,
}

fn main() {
    var p: Pos = Pos {
        x: 2,
        y: "4",
    };
    print(p.x);
    var x: int = p.x;
    var y: str = p.y;
    p.x = p.x + 1;
    p.y = "12";
    print(x, y);
    print(p.x);
}
################################
2
2 4
3
################################
12:18:12:21:Cloned // p.x must be cloned for x
13:18:13:21:Moved // p.y can be moved for y
14:11:14:14:Moved // p.x = p.x + 1