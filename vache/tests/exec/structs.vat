struct Pos {
    x: int,
    y: str,
}

fn main() {
    var p: Pos = Pos {
        x: 2,
        y: "3",
    };
    print(p.x);
    var x: int = p.x;
    var y: str = p.y;
    p.x = p.x + 1;
    p.y = "12";
    print(x, y);
}
################################
2
2 3
################################
12:18:Moved // p.x must be cloned
13:18:Cloned // p.y can be moved