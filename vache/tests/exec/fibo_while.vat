fn fibo(i: int) -> int {
    if i == 0 {
        0
    } else if i == 1 {
        1
    } else {
        var n1: int = 0;
        var n2: int = 1;
        i = i - 1;
        while i > 0 {
            n2 = n2 + n1;
            n1 = n2 - n1;
            i = i - 1;
        }
        n2
    }
}

fn main() {
    print(fibo(0));
    print(fibo(1));
    print(fibo(2));
    print(fibo(11));
}
