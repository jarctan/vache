fn fibo(i: int) -> int {
    if i == 0 {
        1
    } else {
        if i == 1 {
            1
        } else {
            var n1: int = 1;
            var n2: int = 1;
            i = i - 1;
            while i > 0 {
                n2 = n2 + n1;
                n1 = n2 - n1;
                i = i - 1;
            }
            n2
        }
    }
}

fn main() {
    var a: int = 11;
    print(fibo(a));
}
